{
    "timeout": 50000,
    "selectors": [
        {
            "selector": ".show_content",
            "selector_type": "css",
            "selectors": [
                {
                    "selector": "b", 
                    "selector_type": "css", 
                    "separator": "\n"
                }
            ],
            "separator": "\n\n"
        },
        {
            "selector": "pre",
            "selector_type": "css",
            "exclusions": ["font", "a"],
            "replace": [
                {
                    "target_tag": "p",
                    "replace_str": "\n"
                }
            ]
        }
    ],
    "baseurl": "https://www.cool18.com/bbs4/"
}
